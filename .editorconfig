# EditorConfig para fworks.tech
# Mais em: https://editorconfig.org

root = true

[*]
indent_style = space
indent_size = 2
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

[*.md]
trim_trailing_whitespace = false

[*.json]
indent_size = 2

[*.tsx]
indent_size = 2

[*.ts]
indent_size = 2

[*.css]
indent_size = 2

[*.scss]
indent_size = 2
